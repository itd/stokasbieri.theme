<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="stokasbieri Theme">

 <object name="stokasbieri_images"
    meta_type="Filesystem Directory View"
    directory="stokasbieri.theme:skins/stokasbieri_images"/>
 <object name="stokasbieri_templates"
    meta_type="Filesystem Directory View"
    directory="stokasbieri.theme:skins/stokasbieri_templates"/>
 <object name="stokasbieri_styles"
    meta_type="Filesystem Directory View"
    directory="stokasbieri.theme:skins/stokasbieri_styles"/>
 <object name="stokasbieri_js"
    meta_type="Filesystem Directory View"
    directory="stokasbieri.theme:skins/stokasbieri_js"/>

 <skin-path name="stokasbieri Theme" based-on="Plone Default">

 <layer name="stokasbieri_images"
     insert-after="custom"/>
  <layer name="stokasbieri_templates"
     insert-after="stokasbieri_images"/>
  <layer name="stokasbieri_styles"
     insert-after="stokasbieri_templates"/>
  <layer name="stokasbieri_js"
     insert-after="stokasbieri_styles"/>
 </skin-path>

</object>
